{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "Homeservices_backend/src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["Homeservices_backend/src"],
  "exclude": ["node_modules", "dist"]
}


